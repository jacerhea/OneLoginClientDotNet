<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>OneLogin.Client</AssemblyName>
    <RootNamespace>OneLogin</RootNamespace>
    <PackageId>OneLogin.API</PackageId>
    <Description>An client library for accessing all the features of the Onelogin API /1.  https://developers.onelogin.com/api-docs/1/getting-started/dev-overview

This library is NOT for the older v1-v3 API's, which have been deprected.</Description>
    <Authors>Jace Rhea</Authors>
    <PackageProjectUrl>https://github.com/jacerhea/OneLoginClientDotNet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jacerhea/OneLoginClientDotNet</RepositoryUrl>
    <PackageTags>onelogin api client http developers</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Version>1.0.8</Version>
    <AssemblyVersion>1.0.8.0</AssemblyVersion>
    <FileVersion>1.0.8.0</FileVersion>
    <PackageIconUrl>https://www.onelogin.com/assets/img/favicon32.png</PackageIconUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>OneLogin.Client.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>OneLogin.Client.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>
</Project>
